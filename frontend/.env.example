# ============================================
# BuildEstate Frontend - Environment Variables
# ============================================
# Copy this file to .env.local and update values

# Backend API URL
# Local development:
VITE_API_BASE_URL=http://localhost:4000

# Production (your Render backend URL):
# VITE_API_BASE_URL=https://your-backend.onrender.com

# Enable AI Property Hub (set to "true" to enable full AI features)
# Set to "true" for local development, leave unset or "false" for production
VITE_ENABLE_AI_HUB=true
